journal = {Journal of Environmental Economics and Management},
keywords = {Catastrophes,Climate change,Population,Social choice,Uncertainty,Welfare,climate change,discounting,extreme events,population ethics,social welfare,theory,uncertainty},
mendeley-tags = {climate change,discounting,extreme events,population ethics,social welfare,theory,uncertainty},
month = {mar},
number = {2},
pages = {310--325},
publisher = {Elsevier},
title = {{On welfare frameworks and catastrophic climate risks}},
url = {http://linkinghub.elsevier.com/retrieve/pii/S0095069612001209},
volume = {65},
year = {2013}
}
