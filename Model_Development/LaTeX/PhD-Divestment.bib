title = {{What is your life worth?}},
url = {http://www.mitpressjournals.org/doi/abs/10.1162/daed.2008.137.1.49},
volume = {137},
year = {2008}
}
@unpublished{Heitzig2015b,
abstract = {We construct and analyse a conceptual, low-dimensional, deterministic dynamical model of the coevo- lution of the global climate system, energy resources, human population, and economy on time-scales of hundreds to thousands of years, in order to be able to identify, study, and compare possible global coevolutionary pathways related to some of the major physical and socio-economic “planetary bound- aries”. To do so, we first combine existing conceptual models of the global carbon cycle, resource and population dynamics, economic production and growth, energy prices, and technological change into a model consisting of a system of seven generic ordinary differential equations for the main dynamic variables that represent three global carbon stocks, global mean temperature, global population and capital, and the global renewable technology knowledge stock. We then choose simple functional forms for the involved processes of diffusion, greenhouse effect, respiration, photosynthesis, fuel extraction, economic production, consumption, investment, wellbe- ing, population growth, and learning, so that the resulting equations can be transformed into a system of polynomial differential equations since the polynomial form may allow for easier manual or semi- automatic analysis. We reduce the number of free parameters by nondimensionalization and estimate as many remaining parameters as possible at least roughly from the literature. Our model may be extended to study distributional questions by regionalizing some of the variables, which however necessitates further modelling decisions related to, e.g., trade, migration, and capital mobility. copan:GL},
author = {Heitzig, Jobst},
file = {:home/jakob/ownCloud/Papers/Heitzig/Heitzig - 2015 - copan GLOBAL.pdf:pdf},
