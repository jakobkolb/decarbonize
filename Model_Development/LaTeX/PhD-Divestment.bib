journal = {Philosophical transactions. Series A, Mathematical, physical, and engineering sciences},
month = {mar},
number = {1938},
pages = {868--86},
pmid = {21282151},
title = {{Emergent dynamics of the climate-economy system in the Anthropocene.}},
