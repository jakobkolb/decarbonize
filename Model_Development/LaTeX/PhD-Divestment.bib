journal = {Proceedings of the National Academy of Sciences},
keywords = {Computer Simulation,Cultural Evolution,Humans,Income,Models,Population Dynamics,Social Change,Theoretical,coevolution,complexity,conflict,cultural evolution,data analysis},
mendeley-tags = {coevolution,complexity,conflict,cultural evolution,data analysis},
