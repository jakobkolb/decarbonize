@article{Luan2011,
abstract = {Models of decision making are distinguished by those that aim for an optimal solution in a world that is precisely specified by a set of assumptions (a so-called "small world") and those that aim for a simple but satisfactory solution in an uncertain world where the assumptions of optimization models may not be met (a so-called "large world"). Few connections have been drawn between these 2 families of models. In this study, the authors show how psychological concepts originating in the classic signal-detection theory (SDT), a small-world approach to decision making, can be used to understand the workings of a class of simple models known as fast-and-frugal trees (FFTs). Results indicate that (a) the setting of the subjective decision criterion in SDT corresponds directly to the choice of exit structure in an FFT; (b) the sensitivity of an FFT (measured in d') is reflected by the order of cues searched and the properties of cues in an FFT, including the mean and variance of cues' individual d's, the intercue correlation, and the number of cues; and (c) compared with the ideal and the optimal sequential sampling models in SDT and a majority model with an information search component, FFTs are extremely frugal (i.e., do not search for much cue information), highly robust, and well adapted to the payoff structure of a task. These findings demonstrate the potential of theory integration in understanding the common underlying psychological structures of apparently disparate theories of cognition.},
annote = {From a signal detection theoretic point of view there is four clasifications for an answer:
signal correct (hit)
noise correct (correct rejection)
signal false (false positive)
noise false (miss)

conservative decision strategy means less misses but more false alarms.

liberal decision strategy means less false alarms but more misses.



For a given set of cues there is two properties of the structure of possible ffts. first the ordering of the cues and second the exit structure. 
more signal exits early in the tree make its decision bias more conservative,
ordering the cues by their individual d' level froh high to low makes the tree more accurate in general. Cue ordering does not have an effect, if all exits are of the same kind (since then there must be consens between all cues anyway..)},
author = {Luan, Shenghua and Schooler, Lael J and Gigerenzer, Gerd},
doi = {10.1037/a0022684},
file = {:home/jakob/ownCloud/Papers/Luan, Schooler, Gigerenzer/Luan, Schooler, Gigerenzer - 2011 - A signal-detection analysis of fast-and-frugal trees.pdf:pdf},
isbn = {1939-1471 (Electronic)$\backslash$n0033-295X (Linking)},
issn = {0033-295X},
journal = {Psychological review},
keywords = {10,1037,2001,a0022684,city fire,doi,dx,emer-,fast and frugal trees,fast-and-frugal trees,gency medical services division,heuristic decision-making,http,in recollecting september 11,louis cook of the,of the new york,org,signal detection theory,signal-detection theory,supp,supplemental materials,theory integration},
mendeley-tags = {fast and frugal trees,heuristic decision-making,signal detection theory},
number = {2},
pages = {316--338},
pmid = {21381858},
title = {{A signal-detection analysis of fast-and-frugal trees.}},
volume = {118},
year = {2011}
}
